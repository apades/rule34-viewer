{
    "name":"rule34",
    "host":"https://rule34.xx",
    "discover":{
        "url":"https://rule34.xxx/index.php?page=dapi&s=post&q=index&json=1&tags=@{searchString}&limit=@{pageLimit}&pid=@{pageNum}",
        "list":"$",
        "tags":"@js:($)=>$.tags.split(' ')",
        "name":"",
        "cover":"@js:($)=>`https://rule34.xxx/thumbnails/${$.directory}/thumbnail_${$.image.replace(/^(.*)\\..*?$/,'$1.jpg')}`"
    },
    "content":{
        "url":"https://rule34.xxx/index.php",
        "inherit":"discover",
        "image":"@js:({$i})=>`https://rule34.xxx/images/${$i.directory}/${$i.image}`",
        "tags":"@map{copyright::@css:.tag-type-copyright a@text||character::@css:.tag-type-copyright a@text||artist::@css:.tag-type-artist a@text||general::@css:.tag-type-general a@text||metadata::@css:.tag-type-metadata a@text}@"
    }
}