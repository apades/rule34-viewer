{
  "name": "rule34-viewer",
  "version": "1.0.0",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV_NAME=dev expo start\" \"yarn dev_server\" ",
    "dev_a": "concurrently \"cross-env ENV_NAME=dev_a expo start\" \"yarn dev_server\" ",
    "dev_server": "cd proxy_server && yarn dev",
    "and": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "node ./script/build && node ./script/addTag && expo build:android -t apk",
    "server": "cd proxy_server && yarn dev",
    "redux": "set REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?host=localhost&port=19001'\" && yarn dev"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/material-bottom-tabs": "^5.3.9",
    "@react-navigation/native": "^5.8.2",
    "@react-navigation/stack": "^5.11.1",
    "axios": "^0.21.0",
    "expo": "~39.0.2",
    "expo-file-system": "^11.0.2",
    "expo-status-bar": "~1.0.2",
    "lodash": "^4.17.20",
    "node-html-parser": "^2.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
    "react-native-auto-height-image": "^3.2.3",
    "react-native-draggable": "^3.3.0",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-paper": "^4.3.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-super-grid": "^4.0.3",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "0.63.40",
    "@types/react-redux": "^7.1.15",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint-config-apadconfig": "^1.0.4"
  },
  "private": true
}
